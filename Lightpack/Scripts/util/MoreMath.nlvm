package util;

public class MoreMath{

    protected MoreMath(){}

    public static float clamp(float val, float min, float max) {
        return Math.max(min, Math.min(max, val));
    }

    public static int clamp(int val, int min, int max){
        return (int) Math.max(min, Math.min(max, val));
    }
}